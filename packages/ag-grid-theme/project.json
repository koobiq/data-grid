{
    "name": "@koobiq/ag-grid-theme",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/ag-grid-theme",
    "prefix": "lib",
    "tags": [],
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/angular:ng-packagr-lite",
            "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
            "options": {
                "project": "packages/ag-grid-theme/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "packages/ag-grid-theme/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "packages/ag-grid-theme/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        }
    }
}
