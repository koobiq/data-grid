{
    "name": "data-grid",
    "version": "0.0.0",
    "homepage": "https://github.com/koobiq/data-grid",
    "bugs": "https://github.com/koobiq/data-grid/issues",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/koobiq/data-grid.git"
    },
    "packageManager": "yarn@4.1.1",
    "scripts": {
        "prepare": "husky",
        "clean": "nx reset && playwright clear-cache && rimraf .angular dist tmp playwright-report test-results --verbose",
        "-----SERVE-----": "------------------------------------------------------------------------------------------",
        "dev:theme": "nx run ag-grid-angular:serve",
        "-----BUILD-----": "------------------------------------------------------------------------------------------",
        "build:dev:theme": "nx run ag-grid-angular:build",
        "build:theme": "nx run ag-grid-angular-theme:build",
        "build": "nx run-many --target=build",
        "-----GITHUB_RELEASE-----": "---------------------------------------------------------------------------------",
        "release:theme": "nx release --projects=ag-grid-angular-theme --skip-publish",
        "release:theme:preview": "nx release --projects=ag-grid-angular-theme --skip-publish --dry-run",
        "-----LINTERS-----": "----------------------------------------------------------------------------------------",
        "prettier": "prettier --check . --ignore-unknown",
        "prettier:fix": "yarn run prettier --write",
        "eslint": "eslint . --ext='.js,.ts,.html'",
        "eslint:fix": "yarn run eslint --fix",
        "stylelint": "stylelint '**/*.{css,scss}'",
        "stylelint:fix": "yarn run stylelint --fix",
        "lint": "concurrently \"yarn:prettier\" \"yarn:eslint\" \"yarn:stylelint\"",
        "lint:fix": "concurrently \"yarn:prettier:fix\" \"yarn:eslint:fix\" \"yarn:stylelint:fix\"",
        "-----E2E_TESTS-----": "--------------------------------------------------------------------------------------",
        "e2e:setup": "playwright install chromium --with-deps",
        "e2e:dev-ag-grid-angular": "playwright test ./dev/ag-grid-angular"
    },
    "dependencies": {
        "@angular/cdk": "^18.2.13",
        "@angular/common": "^18.2.14",
        "@angular/compiler": "^18.2.14",
        "@angular/core": "^18.2.14",
        "@angular/forms": "^18.2.14",
        "@angular/platform-browser": "^18.2.14",
        "@angular/platform-browser-dynamic": "^18.2.14",
        "@angular/router": "^18.2.14",
        "@fontsource/inter": "^5.2.8",
        "@koobiq/design-tokens": "^3.14.0",
        "@koobiq/icons": "^10.10.0",
        "ag-grid-angular": "^34.3.1",
        "ag-grid-community": "^34.3.1",
        "rxjs": "~7.8.0",
        "zone.js": "~0.16.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.21",
        "@angular-devkit/core": "^18.2.18",
        "@angular-devkit/schematics": "^18.2.21",
        "@angular-eslint/eslint-plugin": "^18.2.0",
        "@angular-eslint/eslint-plugin-template": "^18.2.0",
        "@angular-eslint/template-parser": "^18.2.0",
        "@angular/cli": "^18.2.21",
        "@angular/compiler-cli": "^18.2.14",
        "@angular/language-service": "^18.2.14",
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@nx/angular": "^20.8.3",
        "@playwright/test": "^1.57.0",
        "@schematics/angular": "^18.2.18",
        "@swc-node/register": "^1.11.1",
        "@swc/core": "^1.15.3",
        "@types/eslint": "^8.56.12",
        "@types/node": "^24.10.4",
        "@typescript-eslint/eslint-plugin": "^8.51.0",
        "@typescript-eslint/parser": "^8.51.0",
        "concurrently": "^9.2.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-file-progress": "^1.5.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-rxjs": "^5.0.3",
        "firebase-tools": "^13.35.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.2",
        "ng-packagr": "^18.2.0",
        "nx": "^20.8.3",
        "prettier": "^3.7.4",
        "prettier-plugin-organize-imports": "^4.3.0",
        "rimraf": "^5.0.5",
        "stylelint": "^16.26.1",
        "stylelint-config-standard-scss": "^14.0.0",
        "stylelint-prettier": "^5.0.3",
        "tslib": "^2.3.0",
        "typescript": "5.5.4"
    }
}
