{
    "name": "data-grid",
    "version": "0.0.0",
    "homepage": "https://github.com/koobiq/data-grid",
    "bugs": "https://github.com/koobiq/data-grid/issues",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/koobiq/data-grid.git"
    },
    "engines": {
        "yarn": "^1.22.22",
        "npm": "Please use Yarn instead of NPM to install dependencies.",
        "node": "^18.19.1"
    },
    "packageManager": "yarn@1.22.22",
    "scripts": {
        "prepare": "husky",
        "clean": "nx reset && rimraf .angular dist tmp --verbose",
        "-----SERVE-----": "------------------------------------------------------------------------------------------",
        "dev": "nx run ag-grid-angular:serve --configuration=development",
        "-----BUILD-----": "------------------------------------------------------------------------------------------",
        "build:dev": "nx run ag-grid-angular:build",
        "build:theme": "nx run ag-grid-angular-theme:build",
        "build": "nx run-many --target=build",
        "-----GITHUB_RELEASE-----": "---------------------------------------------------------------------------------",
        "release:theme": "nx release --projects=ag-grid-angular-theme --skip-publish",
        "-----CI-----": "---------------------------------------------------------------------------------------------",
        "prettier": "prettier '**/*.{css,scss,md,yml,json,js,ts,html}' --check",
        "prettier:fix": "yarn run prettier --write",
        "eslint": "eslint . --ext='.js,.ts,.html'",
        "eslint:fix": "yarn run eslint --fix",
        "stylelint": "stylelint '**/*.{css,scss}'",
        "stylelint:fix": "yarn run stylelint --fix"
    },
    "dependencies": {
        "@angular/common": "^18.2.13",
        "@angular/compiler": "^18.2.13",
        "@angular/core": "^18.2.13",
        "@angular/forms": "^18.2.13",
        "@angular/platform-browser": "^18.2.13",
        "@angular/platform-browser-dynamic": "^18.2.13",
        "@angular/router": "^18.2.13",
        "@fontsource/inter": "^5.2.5",
        "@koobiq/design-tokens": "^3.12.1",
        "@koobiq/icons": "^9.4.1",
        "ag-grid-angular": "^30.2.1",
        "ag-grid-community": "^30.2.1",
        "rxjs": "~7.8.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.19",
        "@angular-devkit/core": "^18.2.18",
        "@angular-devkit/schematics": "^18.2.19",
        "@angular-eslint/eslint-plugin": "^18.2.0",
        "@angular-eslint/eslint-plugin-template": "^18.2.0",
        "@angular-eslint/template-parser": "^18.2.0",
        "@angular/cli": "^18.2.19",
        "@angular/compiler-cli": "^18.2.13",
        "@angular/language-service": "^18.2.13",
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@nx/angular": "^20.8.0",
        "@schematics/angular": "^18.2.18",
        "@swc-node/register": "^1.10.10",
        "@swc/core": "^1.11.24",
        "@types/eslint": "^8.56.12",
        "@types/node": "^18.19.100",
        "@typescript-eslint/eslint-plugin": "^8.32.0",
        "@typescript-eslint/parser": "^8.32.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-file-progress": "^1.5.0",
        "eslint-plugin-prettier": "^5.4.0",
        "firebase-tools": "^13.35.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.2",
        "ng-packagr": "^18.2.0",
        "nx": "^20.8.0",
        "prettier": "^3.5.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "rimraf": "^5.0.5",
        "stylelint": "^16.19.1",
        "stylelint-config-standard-scss": "^14.0.0",
        "stylelint-prettier": "^5.0.3",
        "tslib": "^2.3.0",
        "typescript": "5.5.4"
    }
}
